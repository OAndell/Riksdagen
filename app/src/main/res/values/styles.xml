<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.V11.Theme.AppCompat.Light.Dialog">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="animationMenu" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/menuon</item>
        <item name="android:windowExitAnimation">@anim/menuoff</item>
    </style>
</resources>
